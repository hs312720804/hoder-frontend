// 标签的各种样式
>>> .el-tag--yellow
  color #fff
  background-color #f4cf19
  border-color #f4cf19
  .el-tag__close
    color #512DA8

>>> .el-tag--warningOrange
  color #512DA8
  background-color rgba(119, 81, 200, .4)
  border-color rgba(81, 45, 168, .45)
  .el-tag__close
    color #512DA8

>>> .el-tag--warningOrange2
  color: #795548;
  background-color: rgba(167, 130, 117, .5);
  border-color: #7955488c;
  .el-tag__close
    color #512DA8

>>> .el-tag--warningCyan
  color: #00bcd4;
  background-color: rgba(0, 189, 214, .1);
  border-color: #00bcd42b

>>> .el-tag--gray
  color: #fff;
  background-color: rgba(165,155,149, 1);
  border-color: rgba(165,155,149, 1);
  .el-tag__close
    color #fff
    &:hover
      background-color: #666
    
>>> .el-tag--pink
  color: #ff79cd;
  background-color: #fff0f6
  border-color: #fff0f6

>>> .el-tag--gradient
  color: #ffffff;
  border-color: #ffffff;
  background: linear-gradient(315deg, #42d392 25%, #647eff);
  .el-tag__close
    color #fff
    &:hover
      background-color: #666

>>> .el-tag--lavender
  // color: #fff;
  // background-color: #ca9aff
  // border-color: #ca9aff
  color: #ca9aff;
  background-color: #832ce31f;
  border-color: #ca9aff;
  .el-tag__close
    color #ca9aff
    &:hover
      background-color: #666
