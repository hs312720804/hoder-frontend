// 统计分栏布局样式

.wrap-div{
  display: flex;
  justify-content: space-between;
}
.ibox {
  margin-bottom: 25px;
  background-color: #fff;
  border-radius: 4px;
  padding: 15px 20px 20px 20px;
  height: 139px;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.title-one{
  font-size: 12px;
  font-weight: 600;
}
.text-two {
  font-size: 24px;
  color: rgb(26, 179, 148);
  text-align: center;
}
.small-box {
  display: flex;
  justify-content: end;
  flex-direction: column;
}
.small {
  display: inline;
  font-size: 13.3333px;
  display: flex;
  justify-content: space-between;
  align-content: space-around;
  margin-top: 15px;
}
