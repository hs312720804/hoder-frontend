$color0 = #e8e8e8
$color1 = #d9e1ff
$color2 = #e3f9e9
$color3 = #fff5e4
$color4 = #ffd8d2

.d0-color 
  .ibox, .unit-box, .chart-wrap
    background-color: $color0 !important
  

.d1-color 
  .ibox, .unit-box, .chart-wrap
    background: $color1 !important;
  

.d2-color 
  .ibox, .unit-box, .chart-wrap
    background: $color2 !important;
  

.d3-color 
  .ibox, .unit-box, .chart-wrap
    background: $color3 !important;
  

.d4-color 
  .ibox, .unit-box, .chart-wrap
    background: $color4 !important;
  

.d0-sticky-bar-item{
  background: darken($color0, 60%) !important
}
.d1-sticky-bar-item{
  background: darken($color1, 60%) !important
}
.d2-sticky-bar-item{
  background: darken($color2, 60%) !important
}
.d3-sticky-bar-item{
  background: darken($color3, 60%) !important
}
.d4-sticky-bar-item{
  background: darken($color4, 60%) !important
}