.total-wrap
  position: absolute;
  top: 0;
  bottom: 0
  right: 0
  left: 0
>>>.c-scrollbar {
  padding 0
}
.row-wrap
  display: grid;
  grid-template-columns: 200px 200px minmax(0, 1fr);
  grid-template-rows: 100%;
  grid-gap: 16px;
  height 100%
  // margin: -20px;
  // padding: 5px;
  position: relative;
.title
  padding: 10px 16px
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-weight: 500;
  font-size: 16px;
.content {
  height calc(100vh - 130px)
  overflow auto
  box-sizing: border-box;
  position relative
  border-radius: 8px;
}
.icon
  font-size 16px
  width 20px
  line-height: 20px

.lists-item
  font-size 14px
  padding 10px
  display flex
  border-bottom 1px dashed #f3f3f3
  position relative

.target {
  font-size 18px
  text-align: center;
}

.noData {
  background: url('~@/assets/img/noData.svg') center center no-repeat;
