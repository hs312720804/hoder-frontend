  .detail {
    // padding: 15px
  }
  .detail-name {
    font-size 24px
    // color: #676a6c;
    font-family: "open sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-weight: 100;
    position relative
  }
  .detail-name-border {
    position: absolute;
      width: 150%;
      height: 6px;
      bottom: 2px;
      background: linear-gradient(270deg, rgba(248, 115, 115, 0) 0%, #2252d6 100%);
      border-radius: 3px;
      opacity: 0.7;
      bottom: -4px;
  }
  .detail-header {
    display: grid
    grid-template-columns: 35% 53px auto
    // grid-column-gap: 20px
  }
  .detail-header-column {
    // padding: 20px
    box-sizing border-box
    // div {
    //   margin-bottom 20px
    // }
  }
  
  .detail-business-type {
    height: 90px;
    width: 100%;
    background: #fff;
    margin-top: 15px;
    position: relative
    display: flex;
    justify-content: center;
    align-items: center;
    color: #000;
    border-radius 4px
  }
  .kpi-wrap {
    display grid;
    grid-template-columns: 45px auto
  }
  .detail-kpi {
    height: 200px;
    width: 100%;
    // border 1px solid #ccc
    background: #fff;
    font-size 14px
    padding: 15px;
    box-sizing: border-box;
    line-height: 14px;
    color #000
    border-radius: 4px
  }
  .detail-kpi-table {
    width: 100%;
    display grid
    grid-template-columns: 50% 50%
    grid-template-rows: 35px 35px
    grid-column-gap: 20px
  }
  .set-start, .set-end {
    min-height 115px
    // border 1px solid #ccc
    // background: #f4f4f5;
    position: relative
    padding-bottom: 50px
    // overflow: auto;
    border-radius 4px
  }
  .box-fotter {
    position: absolute;
    bottom: 0;
    padding: 10px;
    display: grid;
    /* justify-content: center; */
    /* grid-template-columns: 100%; */
    /* align-items: center; */
    /* grid-template-rows: auto; */
    width: 100%;
    box-sizing: border-box;
  }

  .d-info {
    color: rgba(255,255,255,0.5);
    /* padding: 8px 16px; */
    font-size: 13px;
    display: grid;
    grid-template-columns: 83px auto;
    grid-template-rows: repeat(3, 30px);
    -webkit-box-align: center;
    -ms-flex-align: center;
    align-items: center;
    margin: 20px 0 39px 0;
    width: 68%;
  }

