  .outer-and {
    margin-left: 70px;
    position: absolute;
    top: 10px;
    right: 0;
    bottom: 3px;
    left: 0;
    width: 3px;
    height: auto;
    margin: auto 10px;
    border: 1px dashed #adb1f4
    border-right: 0;
    z-index: 999;
    display: flex;
    align-items: center;
    justify-content: center;
  }
  .and-or {
    padding: 4px 6px
    color: #FFF;
    background-color: #67c23a;
    border-color: #67c23a;
    border-radius 20px
    font-size 12px
  }

  .d-info >>> .el-input__inner {
    background: #07133c;
