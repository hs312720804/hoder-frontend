    /* background-color: #ffffff; */
    /* -webkit-box-shadow: 0 0 3px 0 #4a54ff; */
    /* box-shadow: 0 0 3px 0 #4a54ff; */
  }

  .d-info >>> .el-input__inner {
    background: #07133c;
    color: #fff;
  }
  .rule-string 
    >>> .label-item, >>> .label-ground, >>> .bav-attr-warp, >>> .child-attr-wrap
      border none
  
  .lists-item >>> .el-dropdown
    color: #fff
  .gray-row
    color gray

  .detail-kpi
    border: 1px solid rgba(25,186,139,.17)
    background url(../../assets/img/index.png)
    color #fff
    box-shadow: 0 0 6px #181c56 inset
    &:before
      position: absolute;
      top: 0;
      left: 0;
      content: "";
      width: 10px;
      height: 10px;
      border-top: 2px solid $borderColor;
      border-left: 2px solid $borderColor;
    &:after
      position: absolute;
      top: 0;
      right: 0;
      content: "";
      width: 10px;
      height: 10px;
      border-top: 2px solid $borderColor;
      border-right: 2px solid $borderColor;
  .panel-footer
    position: absolute;
    left: 0;
    bottom: 0;
    width: 100%;
    &:before
      position: absolute;
      bottom: 0;
      left: 0;
      content: "";
      width: 10px;
      height: 10px;
      border-bottom: 2px solid $borderColor;
      border-left: 2px solid $borderColor;
    &:after
      position: absolute;
      bottom: 0;
      right: 0;
      content: "";
      width: 10px;
      height: 10px;
      border-bottom: 2px solid $borderColor;
      border-right: 2px solid $borderColor;
  .bav-wrap {
    >>> .el-input__inner {
      background-color: #09163c;
    }
  }
