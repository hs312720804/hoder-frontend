  .sceneList-wrap
    position: absolute;
    width: 100%;
    bottom: 53px;
    top: 83px;
    overflow: auto;
  .info-class {
    // box-sizing: border-box;
    // overflow: hidden;
    transition: opacity .2s;
    // color: #131e0e;
    margin: 15px 10px;
    margin-right: 0
    font-size: 14px
    word-break: break-word;
    display grid
    grid-template-columns: auto 30px;
    // align-items: top
  }
  .border-line{
    padding: 8px 16px 8px 26px
    // border-radius: 4px;
    // border: 1px solid #67c23a;
